<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /cvs

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^([a-zA-Z0-9_\-\/\.]+)\?(.*)$ index.php?f=$1&$2=$3 [T=application/x-httpd-php,L]

    RewriteCond %{REQUEST_FILENAME}  !-d
    RewriteCond %{REQUEST_FILENAME}  !-f
    RewriteRule ^(.*)$ index.php?f=$1 [QSA]
</IfModule>

